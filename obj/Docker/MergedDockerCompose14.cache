name: dockercompose12452298340606706741
services:
  apidotnet:
    build:
      context: C:\C#\DotNetCore
      dockerfile: Data/Dockerfile
    depends_on:
      db:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Production
      TZ: America/Sao_Paulo
    image: apidotnet
    networks:
      udemy-network: null
    ports:
    - mode: ingress
      target: 80
      published: "7101"
      protocol: tcp
  db:
    build:
      context: C:\C#\DotNetCore
      dockerfile: Data/Dockerfile
    command:
    - mysqld
    - --default-authentication-plugin=mysql_native_password
    depends_on:
      db:
        condition: service_started
    environment:
      MYSQL_DATABASE: dotnet
      MYSQL_PASSWORD: docker
      MYSQL_ROOT_PASSWORD: docker
      MYSQL_USER: docker
      TZ: America/Sao_Paulo
    image: apidotnet-db
    networks:
      udemy-network: null
    ports:
    - mode: ingress
      target: 3306
      published: "3308"
      protocol: tcp
    restart: always
networks:
  udemy-network:
    name: dockercompose12452298340606706741_udemy-network
    driver: bridge